<div class='container'>
  <h1 class='title'>Liste des medecins</h1>
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col"></th>
        <th scope="col">Prénom</th>
        <th scope="col">Statut</th>
        <th scope="col">Cout(€)</th>
        <th scope="col"></th>
      </tr>
    </thead>
    <tbody>
      <% @workers.each do |worker| %>
        <%= render 'worker_info', worker: worker %>
      <% end %>
    </tbody>
  </table>
  <%= link_to 'retour à l\'accueil', root_path, class: 'btn btn-primary button' %>
</div>
